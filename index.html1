<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Winamp Player</title>
    <script src="https://unpkg.com/webamp"></script> <!-- This loads Webamp.js -->
</head>
<body>
    <h1>Winamp Test</h1>
    <div id="winamp-container"></div>

    <script>
        const webamp = new Webamp({
            initialTracks: [],
            initialSkin: {
                url: "https://raw.githubusercontent.com/amelazmarino/winamp-host/main/Windows_9x.wsz"
            }
        });

        webamp.renderWhenReady(document.getElementById("winamp-container"));
    </script>
</body>
</html>
